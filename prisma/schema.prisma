datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Document {
  id         Int      @id @default(autoincrement())
  fileName   String
  fileUrl    String
  docName    String
  docRev     String
  uploadedAt DateTime @default(now())
}
